int fifoCreate(void)
{

    int shmid = pshmget(
# 35 "/home/diana/Desktop/3ano/SO/bounded-buffer/fifo_unsafe.cpp" 3 4
                       ((__key_t) 0)
# 35 "/home/diana/Desktop/3ano/SO/bounded-buffer/fifo_unsafe.cpp"
                                  , sizeof(FIFO), 0600 | 
# 35 "/home/diana/Desktop/3ano/SO/bounded-buffer/fifo_unsafe.cpp" 3 4
                                                         01000 
# 35 "/home/diana/Desktop/3ano/SO/bounded-buffer/fifo_unsafe.cpp"
                                                                   | 
# 35 "/home/diana/Desktop/3ano/SO/bounded-buffer/fifo_unsafe.cpp" 3 4
                                                                     02000
# 35 "/home/diana/Desktop/3ano/SO/bounded-buffer/fifo_unsafe.cpp"
                                                                             );


    FIFO * fifo = (FIFO*)pshmat(shmid, 
# 38 "/home/diana/Desktop/3ano/SO/bounded-buffer/fifo_unsafe.cpp" 3 4
                                      __null
# 38 "/home/diana/Desktop/3ano/SO/bounded-buffer/fifo_unsafe.cpp"
                                          , 0);


    uint32_t i;
    for (i = 0; i < 5; i++)
    {
        fifo->slot[i].id = 99999999;
        fifo->slot[i].value = 99999999;
    }
    fifo->ii = fifo->ri = 0;
    fifo->cnt = 0;


    pshmdt(fifo);
    return shmid;
}



